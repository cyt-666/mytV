{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "mytv",
  "version": "0.1.0",
  "identifier": "com.mytv.app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "mytv",
        "width": 1200,
        "height": 800,
        "decorations": false,
        "shadow": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {}
}
